    0% {transform: translateY(0) rotateX(0);}
    25% {transform: translateY(-30px) rotateX(40deg);}
    50% {transform: rotateX(0) translateY(0);}
    75% {transform: rotateX(-40deg) translateY(30px);}
    100% {transform: rotateX(0) translateY(0);}
}

